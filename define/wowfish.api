syntax = "v1"

type WithdrawReqs {
	Reqs string `form:"accounts"`
}

type WithdrawResp {
	Result bool `json:"result"`
}

type NftQueryIdsReq {
	Address string `form:"address"`
}

type NftQueryIdsResp {
	Ids []int64 `form:"ids"`
}

@server(
	group: chain
	prefix: /chain
)

service chain {
	@handler withdraw
	post /withdraw(WithdrawReqs) returns(WithdrawResp)

	@handler queryNft
	post /queryNft(NftQueryIdsReq) returns(NftQueryIdsResp)

}